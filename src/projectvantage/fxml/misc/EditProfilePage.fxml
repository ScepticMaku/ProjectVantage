      </ImageView>
      <Label fx:id="usernamePlaceholder" alignment="CENTER" layoutX="14.0" layoutY="191.0" prefHeight="18.0" prefWidth="214.0" stylesheets="@../../css/title-label-style.css" text="Username" />
