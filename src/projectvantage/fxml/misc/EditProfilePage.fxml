<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="324.0" prefWidth="527.0" stylesheets="@../../css/style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectvantage.controllers.misc.EditProfilePageController">
