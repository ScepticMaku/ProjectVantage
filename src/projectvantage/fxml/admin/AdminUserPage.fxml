      </ImageView>
      <Label fx:id="usernamePlaceholder" alignment="CENTER" layoutX="57.0" layoutY="206.0" prefHeight="18.0" prefWidth="160.0" text="Username" />
      <Label layoutX="239.0" layoutY="103.0" text="First name:" textFill="#2f9efe">
