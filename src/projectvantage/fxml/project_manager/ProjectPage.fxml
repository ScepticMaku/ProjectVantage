<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectvantage.controllers.project_manager.ProjectPageController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Projects" />
      <Pagination fx:id="pagination" layoutY="69.0" pageCount="50" prefHeight="497.0" prefWidth="600.0" />
      <TableView fx:id="projectTable" layoutX="14.0" layoutY="75.0" onMouseClicked="#userTableMouseClickHandler" prefHeight="449.0" prefWidth="572.0">
         <columns>
            <TableColumn fx:id="idColumn" editable="false" prefWidth="50.0" sortable="false" text="ID" />
            <TableColumn fx:id="nameColumn" editable="false" prefWidth="163.0" sortable="false" text="Name" />
            <TableColumn fx:id="duedateColumn" editable="false" prefWidth="147.0" sortable="false" text="Due date" />
            <TableColumn fx:id="statusColumn" editable="false" prefWidth="123.0" sortable="false" text="Status" />
            <TableColumn fx:id="actionColumn" prefWidth="73.0" text="Action" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </TableView>
      <Button fx:id="addProjectButton" layoutX="508.0" layoutY="39.0" mnemonicParsing="false" onMouseClicked="#addProjectButtonMouseClickHandler" text="Add Project" />
      <TextField layoutX="14.0" layoutY="39.0" />
      <Button layoutX="173.0" layoutY="39.0" mnemonicParsing="false" text="Button" />
      <Button layoutX="417.0" layoutY="39.0" mnemonicParsing="false" text="View Project" />
   </children>
</AnchorPane>
